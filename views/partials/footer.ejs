</main>
<script>
<% if (typeof success !== "undefined" && success.length > 0) { %>
      <% success.forEach(msg => { %>
        showToast("success", "Success", "<%= msg %>");
      <% }) %>
    <% } %>

    <% if (typeof error !== "undefined" && error.length > 0) { %>
      <% error.forEach(msg => { %>
        showToast("error", "Error", "<%= msg %>");
      <% }) %>
    <% } %>
  </script>

